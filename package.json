{
  "name": "ol3-popup",
  "version": "5.1.3",
  "description": "OpenLayers Popup",
  "main": "built/index.js",
  "scripts": {
    "start": "parcel ./rawgit.html",
    "build": "parcel build --public-url . rawgit.html",
    "clean": "rmdir .\\built /s/q & del test\\*.js /s",
    "uglify": "pushd built && uglifyjs index.max.js --source-map \"content='index.max.js.map',url='index.js.map'\" --output=index.js --compress --mangle && popd",
    "build-for-rawgit": "tsc -p tsconfig-examples.json",
    "live": "tsc rawgit.ts -m es2015 && live-server --port=5000 --wait=200 --watch=./built/*.js --open=rawgit.html?debug=1 --browser=CHROME",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca0v/ol3-popup.git"
  },
  "keywords": [
    "OpenLayers"
  ],
  "author": "coreyalix@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ca0v/ol3-popup/issues"
  },
  "homepage": "https://github.com/ca0v/ol3-popup#readme",
  "devDependencies": {
    "@types/jquery": "^3.3.6",
    "@types/ol": "^4.6.2",
    "@types/requirejs": "^2.1.31",
    "live-server": "^1.2.0",
    "typescript": "^3.0.1",
    "uglify-js": "^3.4.8"
  },
  "dependencies": {
    "ol": "^5.1.3",
    "ol3-fun": "https://github.com/ca0v/ol3-fun/tarball/v5.1.3",
    "ol3-symbolizer": "https://github.com/ca0v/ol3-symbolizer/tarball/v5.1.3",
    "requirejs": "^2.3.5",
    "xstyle": "^0.3.2"
  }
}
